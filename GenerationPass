with open("passB.txt", "w+") as file:
        i = 3
        while i <= 8:
            for x in permutations(["1","2","3","8","9","9","almz","ALMZ"], i):
                if len("".join(x)) >= 6 and x.count("1") <= 1 and x.count("2") <= 1 and x.count("3") <= 1 and  x.count("3") <= 1 and x.count("8") <= 1 and x.count("9") <= 2:
                    file.write(''.join(x) + "\n")
            i += 1
